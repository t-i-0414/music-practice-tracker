{
  "name": "@music-practice-tracker/admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "gen:api-types": "openapi-typescript http://localhost:3001/api-json -o generated/types/api.d.ts",
    "lint:check": "next lint",
    "lint:fix": "next lint --fix",
    "start:dev": "next dev --turbopack",
    "start:prod": "next start",
    "test:unit": "vitest run --config tests/unit/vitest.config.ts",
    "test:unit:cov": "bun run test:unit --coverage",
    "test:unit:ui": "bun run test:unit:watch --ui",
    "test:unit:watch": "vitest --config tests/unit/vitest.config.ts",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.3.3"
  },
  "devDependencies": {
    "@music-practice-tracker/eslint-configs": "workspace:*",
    "eslint-config-next": "15.3.3"
  }
}
