{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "admin-api": {
      "type": "application",
      "entryFile": "admin-api.main"
    },
    "app-api": {
      "type": "application",
      "entryFile": "app-api.main"
    }
  }
}
