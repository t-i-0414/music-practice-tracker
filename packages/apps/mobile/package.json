{
  "name": "@music-practice-tracker/mobile",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "build-storybook": "storybook build",
    "gen:api-types": "bun scripts/gen.api-types.ts",
    "lint:es:check": "eslint",
    "lint:es:deadcode:check": "ts-prune -e",
    "lint:es:fix": "eslint --fix",
    "maestro:install": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "reset-project": "node ./scripts/reset-project.js",
    "start:dev": "expo start",
    "start:dev:android": "expo start --android",
    "start:dev:ios": "expo start --ios",
    "start:dev:web": "expo start --web",
    "storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --port 6007",
    "storybook:android": "bun run storybook --android",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:ios": "bun run storybook --ios",
    "storybook:web": "storybook dev -p 6008",
    "test": "run-s test:unit test:e2e",
    "test:e2e": "make test-e2e",
    "test:e2e:ui": "make test-e2e-ui",
    "test:unit": "jest --config tests/unit/jest.config.ts",
    "test:unit:cov": "bun run test:unit --coverage",
    "test:unit:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:unit:watch": "bun run test:unit --watch",
    "type:check": "run-p type:check:base type:check:storybook",
    "type:check:base": "tsc --noEmit",
    "type:check:storybook": "tsc --noEmit -p tsconfig.storybook.json"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@storybook/addon-ondevice-actions": "^9.0.18",
    "@storybook/addon-ondevice-backgrounds": "^9.0.18",
    "@storybook/addon-ondevice-controls": "^9.0.18",
    "@storybook/addon-ondevice-notes": "^9.0.18",
    "expo": "^53.0.19",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-runtime": "~5.0.4",
    "@storybook/react": "^9.0.16",
    "@storybook/react-native": "^9.0.14",
    "@storybook/react-native-web-vite": "^9.0.16",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/babel__core": "~7.20.5",
    "@types/react-native-web": "~0.19.1",
    "babel-preset-expo": "~13.0.0",
    "eslint-config-expo": "~9.2.0"
  }
}
