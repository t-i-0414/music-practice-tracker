  },
  "license": "UNLICENSED",
  "author": "Takuya Iwashiro <takuya.iwashiro@takudev.net>",
  "workspaces": [
    "packages/apps/*",
    "packages/libs/*"
  ],
  "scripts": {
    "changeset:add": "changeset",
    "changeset:version": "changeset version",
    "cspell": "cspell '**'",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && bun run format:prisma",
    "format:prisma": "cd packages/apps/backend && bun run prisma:format && cd -",
    "lint:dotenv:check": "./bin/dotenv-linter",
    "lint:dotenv:compare": "./bin/dotenv-linter compare .env .env.*",
    "lint:dotenv:fix": "./bin/dotenv-linter fix",
    "lint:es:check": "eslint && sh scripts/lint-es.sh",
    "lint:es:deadcode:check": "sh scripts/check-deadcode.sh",
    "lint:es:fix": "eslint --fix && sh scripts/lint-es.sh --fix",
    "lint:markdown:check": "markdownlint '**/*.md' --ignore '**/node_modules/**'",
    "lint:markdown:fix": "markdownlint '**/*.md' --ignore '**/node_modules/**' --fix",
    "lint:secret:check": "secretlint '**/*'",
    "lint:secret:fix": "secretlint --fix '**/*'",
    "lint:shell:check": "sh scripts/lint-shell.sh",
    "type:check": "tsc --noEmit && sh scripts/type_check.sh"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "dotenv": "^17.2.1",
    "react": "19.0.0",
