{
  "name": "music-practice-tracker",
  "version": "1.0.0",
  "workspaces": [
    "packages/apps/*",
    "packages/libs/*"
  ],
  "scripts": {
    "ci:temp": "run-s cspell format:check lint:check markdown:check secretlint:check type:check test",
    "cspell": "cspell '**'",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && bun run format:prisma",
    "format:prisma": "cd packages/apps/backend && bun run prisma:format && cd -",
    "lint:check": "sh scripts/lint.sh",
    "lint:fix": "sh scripts/lint.sh --fix",
    "markdown:check": "markdownlint '**/*.md' --ignore '**/node_modules/**'",
    "markdown:fix": "markdownlint '**/*.md' --ignore '**/node_modules/**' --fix",
    "secretlint:check": "secretlint '**/*'",
    "secretlint:fix": "secretlint --fix '**/*'",
    "test": "sh scripts/test.sh",
    "test:cov": "sh scripts/test.sh --coverage",
    "type:check": "sh scripts/type_check.sh"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "dotenv": "^16.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.2",
    "@secretlint/secretlint-rule-preset-recommend": "^10.2.0",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/nextjs-vite": "^9.0.16",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.1",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.8",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "~21.1.7",
    "@types/node": "22.15.31",
    "@types/react": "^19.1.7",
    "@types/react-dom": "^19.1.6",
    "@types/source-map-support": "^0.5.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/rule-tester": "^8.34.1",
    "@typescript-eslint/utils": "^8.34.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.2.4",
    "cspell": "^9.0.2",
    "cypress": "^14.5.1",
    "dotenv-cli": "^7.4.4",
    "dotenv-linter": "^0.2.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-storybook": "^9.0.16",
    "eslint-plugin-testing-library": "^7.5.3",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "jest-mock-extended": "^4.0.0",
    "jsdom": "^26.1.0",
    "lefthook": "^1.11.13",
    "majestic": "^1.8.1",
    "markdownlint-cli": "^0.45.0",
    "msw": "^2.10.3",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.8.0",
    "playwright": "^1.53.2",
    "prettier": "^3.5.3",
    "secretlint": "^10.2.0",
    "source-map-support": "^0.5.21",
    "storybook": "^9.0.16",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
